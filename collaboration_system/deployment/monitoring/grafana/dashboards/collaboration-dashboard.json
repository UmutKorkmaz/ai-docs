{
  "dashboard": {
    "id": null,
    "title": "AI Documentation Collaboration System",
    "tags": ["collaboration", "ai-docs"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Active Users",
        "type": "stat",
        "targets": [
          {
            "expr": "active_users_total",
            "legendFormat": "Active Users"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto",
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "textMode": "auto"
        },
        "fieldConfig": {
          "defaults": {
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 100},
                {"color": "red", "value": 500}
              ]
            },
            "unit": "none"
          }
        }
      },
      {
        "id": 2,
        "title": "Collaboration Sessions",
        "type": "stat",
        "targets": [
          {
            "expr": "active_collaboration_sessions",
            "legendFormat": "Active Sessions"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto",
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "textMode": "auto"
        }
      },
      {
        "id": 3,
        "title": "Real-time Message Throughput",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(websocket_messages_total[5m])",
            "legendFormat": "Messages/sec"
          }
        ],
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 8},
        "options": {
          "legend": {"displayMode": "list", "placement": "bottom"},
          "tooltip": {"mode": "single", "sort": "none"}
        }
      },
      {
        "id": 4,
        "title": "API Response Times",
        "type": "timeseries",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, http_request_duration_seconds_bucket)",
            "legendFormat": "50th percentile"
          },
          {
            "expr": "histogram_quantile(0.95, http_request_duration_seconds_bucket)",
            "legendFormat": "95th percentile"
          },
          {
            "expr": "histogram_quantile(0.99, http_request_duration_seconds_bucket)",
            "legendFormat": "99th percentile"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
        "options": {
          "legend": {"displayMode": "list", "placement": "bottom"},
          "tooltip": {"mode": "single", "sort": "none"}
        },
        "yAxis": {
          "label": "Response Time (seconds)"
        }
      },
      {
        "id": 5,
        "title": "API Error Rate",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(http_requests_total{status=~\"5..\"}[5m])",
            "legendFormat": "5xx Errors"
          },
          {
            "expr": "rate(http_requests_total{status=~\"4..\"}[5m])",
            "legendFormat": "4xx Errors"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16},
        "options": {
          "legend": {"displayMode": "list", "placement": "bottom"},
          "tooltip": {"mode": "single", "sort": "none"}
        },
        "yAxis": {
          "label": "Requests/sec"
        }
      },
      {
        "id": 6,
        "title": "Database Performance",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(postgresql_queries_total[5m])",
            "legendFormat": "Queries/sec"
          },
          {
            "expr": "rate(postgresql_slow_queries_total[5m])",
            "legendFormat": "Slow Queries/sec"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24},
        "options": {
          "legend": {"displayMode": "list", "placement": "bottom"},
          "tooltip": {"mode": "single", "sort": "none"}
        }
      },
      {
        "id": 7,
        "title": "Database Connections",
        "type": "timeseries",
        "targets": [
          {
            "expr": "postgresql_connections_total",
            "legendFormat": "Total Connections"
          },
          {
            "expr": "postgresql_connections_active",
            "legendFormat": "Active Connections"
          },
          {
            "expr": "postgresql_connections_idle",
            "legendFormat": "Idle Connections"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 24},
        "options": {
          "legend": {"displayMode": "list", "placement": "bottom"},
          "tooltip": {"mode": "single", "sort": "none"}
        }
      },
      {
        "id": 8,
        "title": "Redis Memory Usage",
        "type": "timeseries",
        "targets": [
          {
            "expr": "redis_memory_used_bytes",
            "legendFormat": "Used Memory"
          },
          {
            "expr": "redis_memory_max_bytes",
            "legendFormat": "Max Memory"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 32},
        "options": {
          "legend": {"displayMode": "list", "placement": "bottom"},
          "tooltip": {"mode": "single", "sort": "none"}
        },
        "yAxis": {
          "label": "Bytes",
          "unit": "bytes"
        }
      },
      {
        "id": 9,
        "title": "System Resources",
        "type": "timeseries",
        "targets": [
          {
            "expr": "100 - (avg by(instance) (rate(node_cpu_seconds_total{mode=\"idle\"}[5m])) * 100)",
            "legendFormat": "CPU Usage %"
          },
          {
            "expr": "(node_memory_MemTotal_bytes - node_memory_MemAvailable_bytes) / node_memory_MemTotal_bytes * 100",
            "legendFormat": "Memory Usage %"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 32},
        "options": {
          "legend": {"displayMode": "list", "placement": "bottom"},
          "tooltip": {"mode": "single", "sort": "none"}
        },
        "yAxis": {
          "label": "Usage %",
          "max": 100,
          "min": 0
        }
      },
      {
        "id": 10,
        "title": "User Activity Heatmap",
        "type": "heatmap",
        "targets": [
          {
            "expr": "sum by (hour) (rate(user_actions_total[1h]))",
            "legendFormat": "Actions per Hour"
          }
        ],
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 40},
        "options": {
          "color": {
            "mode": "opacity"
          },
          "legend": {"displayMode": "list", "placement": "bottom"},
          "tooltip": {"mode": "single", "sort": "none"}
        }
      },
      {
        "id": 11,
        "title": "Document Activity",
        "type": "table",
        "targets": [
          {
            "expr": "topk(10, sum by (document_id) (rate(document_edits_total[1h])))",
            "legendFormat": "Edits per Hour"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 48},
        "options": {
          "showHeader": true,
          "footer": {"show": false},
          "sortBy": [
            {"desc": true, "displayName": "Value"}
          ]
        },
        "fieldConfig": {
          "defaults": {
            "custom": {
              "align": "auto",
              "displayMode": "auto"
            }
          }
        }
      },
      {
        "id": 12,
        "title": "WebSocket Connection Stats",
        "type": "timeseries",
        "targets": [
          {
            "expr": "websocket_connections_active",
            "legendFormat": "Active Connections"
          },
          {
            "expr": "rate(websocket_connections_created_total[5m])",
            "legendFormat": "New Connections/sec"
          },
          {
            "expr": "rate(websocket_connections_dropped_total[5m])",
            "legendFormat": "Dropped Connections/sec"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 48},
        "options": {
          "legend": {"displayMode": "list", "placement": "bottom"},
          "tooltip": {"mode": "single", "sort": "none"}
        }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"],
      "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "2d", "7d", "30d"]
    },
    "templating": {
      "list": []
    },
    "annotations": {
      "list": [
        {
          "builtIn": 1,
          "datasource": "-- Grafana --",
          "enable": true,
          "hide": true,
          "iconColor": "rgba(0, 211, 255, 1)",
          "name": "Annotations & Alerts",
          "type": "dashboard"
        }
      ]
    },
    "refresh": "5s",
    "schemaVersion": 27,
    "style": "dark",
    "tags": ["collaboration", "monitoring"],
    "timezone": "browser",
    "title": "AI Documentation Collaboration System",
    "uid": "collaboration-dashboard",
    "version": 1,
    "weekStart": ""
  }
}