    "    print(f\"Prompt: {prompt}\")\n",
    "    print(f\"\\nResponse:\")\n",
    "    print(response)\n",
    "    print(f\"\\nContains sentiment classification: {few_shot_results[technique]['has_sentiment']}\")\n",
    "    print(f\"Follows format: {few_shot_results[technique]['has_format']}\")\n",
    "    print(f\"{'='*60}\")"
   ]
